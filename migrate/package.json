{
  "name": "mooncaketv-migrations",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Database migrations for MoonCakeTV",
  "scripts": {
    "migrate:up": "node-pg-migrate up --config-file migrations.json",
    "migrate:down": "node-pg-migrate down --config-file migrations.json",
    "migrate:status": "node-pg-migrate status --config-file migrations.json"
  },
  "dependencies": {
    "node-pg-migrate": "^8.0.3",
    "pg": "^8.16.3",
    "bcryptjs": "^3.0.2"
  }
}
